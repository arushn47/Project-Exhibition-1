{
  "env": {
    "PYTHON_VERSION": "3.11.5"
  },
    "builds": [
      {
        "src": "requirements.txt",
        "use": "@vercel/python",
        "config": {
          "pythonVersion": "3.12"
        }
      }
    ]
  ,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      },
      {
        "src": "requirements.txt",
        "use": "@vercel/python",
        "config": {
          "pythonVersion": "3.12"
        }
      }

    ]
    
}
